{% extends 'site/layout.html.twig' %}

{% block title 'Passé une annonce gratuite - Sélectionner une catégorie' %}

{% block meta %}
    <meta name="google" content="nositelinkssearchbox">
{% endblock %}

{% block app_flashes %}{% endblock %}

{% block app_intro %}
    <section class="bc container">
        <div class="pb-0 mb-0">
            {{ wo_render_breadcrumbs({ viewTemplate: '_breadcrumb.html.twig', listClass: 'breadcrumb pb-lg-2 pb-0',
            itemClass: 'breadcrumb-item', separator: '>', separatorClass: 'fas fa-long-arrow-alt-right'}) }}
        </div>
    </section>
{% endblock %}

{% block app_body %}
    {{ include('site/category/switch/content.html.twig') }}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}

    {% include 'ui/_javascripts.html.twig' with {'path': 'assets/js/site/category.js'} %}
{% endblock %}




