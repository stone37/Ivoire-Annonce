<div class="form-row">
    <div class="col-12">{{ form_row(form.name) }}</div>
    <div class="col-12 col-md-6">{{ form_row(form.email) }}</div>
    <div class="col-12 col-md-6">{{ form_row(form.phone) }}</div>
</div>
<div class="form-row">
    <div class="col-12">
        <div class="md-form md-outline">
            {{ form_widget(form.content) }}
        </div>
    </div>
    <div class="col-12">{{ form_row(form.recaptchaToken) }}</div>
</div>

